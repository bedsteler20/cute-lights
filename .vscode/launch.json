{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/local/bin/cute_lights",
            "console": "integratedTerminal",
            "preLaunchTask": "build",
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}/src",
                    "remoteRoot": "/usr/local/share/cute_lights/cute_lights"
                }
            ]
        }
    ],
    "compounds": []
}
